{
  "name": "SecretContent",
  "type": "dmss://system/SIMOS/Blueprint",
  "description": "Blueprint for generic secret string value",
  "attributes": [
    {
      "name": "type",
      "type": "dmss://system/SIMOS/BlueprintAttribute",
      "attributeType": "string"
    },
    {
      "name": "value",
      "type": "dmss://system/SIMOS/BlueprintAttribute",
      "description": "Value of the secret. AES encrypted if 'encrypted' is true, plain text if not",
      "attributeType": "string"
    },
    {
      "name": "encrypted",
      "type": "dmss://system/SIMOS/BlueprintAttribute",
      "attributeType": "boolean",
      "optional": true,
      "default": false
    }
  ]
}
