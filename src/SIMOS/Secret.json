{
  "name": "Secret",
  "type": "dmss://system/SIMOS/Blueprint",
  "description": "Blueprint for generic secret string value",
  "attributes": [
    {
      "name": "type",
      "type": "dmss://system/SIMOS/BlueprintAttribute",
      "attributeType": "string"
    },
    {
      "name": "name",
      "type": "dmss://system/SIMOS/BlueprintAttribute",
      "description": "globaly unique name of the secret",
      "attributeType": "string"
    },
    {
      "name": "content",
      "type": "dmss://system/SIMOS/BlueprintAttribute",
      "description": "Value of the secret. AES encrypted if 'encrypted' is true, plain text if not",
      "attributeType": "./SecretContent",
      "contained": true
    },
    {
      "name": "description",
      "type": "dmss://system/SIMOS/BlueprintAttribute",
      "optional": true,
      "attributeType": "string"
    },
    {
      "name": "created",
      "type": "dmss://system/SIMOS/BlueprintAttribute",
      "optional": true,
      "attributeType": "string"
    },
    {
      "name": "timeToLiveDays",
      "type": "dmss://system/SIMOS/BlueprintAttribute",
      "optional": true,
      "attributeType": "integer",
      "default": 90
    }
  ]
}
